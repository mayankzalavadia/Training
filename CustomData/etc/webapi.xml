<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/customdata/getcustomdata" method="GET">
        <service class="Training\CustomData\Api\LoadByIDInterface" method="getCollection"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/customdata/getbyid/:id" method="GET">
        <service class="Training\CustomData\Api\LoadByIDInterface" method="loadById"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
